{
  "name": "@apexdevtools/benchmarker",
  "version": "4.0.0",
  "description": "Benchmarks performance of processes on Salesforce Orgs",
  "author": {
    "name": "Apex Dev Tools Team",
    "email": "apexdevtools@gmail.com",
    "url": "https://github.com/apex-dev-tools"
  },
  "repository": "apex-dev-tools/benchmarker",
  "homepage": "https://github.com/apex-dev-tools/benchmarker",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "npm run build:clean && npm run compile",
    "build:clean": "rm -rf dist",
    "compile": "tsc",
    "compile:apex": "tsx ./scripts/inlineApex.ts",
    "doc:generate": "typedoc --out docs/api ./src",
    "lint": "eslint ./src ./test ./test_system --fix",
    "prepare": "husky",
    "postinstall": "patch-package",
    "postversion": "npm run doc:generate",
    "test": "nyc mocha --timeout 10000 'test/**/*.test.*'",
    "test:only": "nyc mocha --timeout 10000",
    "test:system:init": "cp test_system/config/.env .env",
    "test:system": "mocha --timeout 30000 'test_system/**/*.test.*'"
  },
  "files": [
    "db/migrations",
    "dist/src",
    "docs/user",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@apexdevtools/apex-parser": "^5.0.0-beta.4",
    "@jsforce/jsforce-node": "^3.10.3",
    "@salesforce/core": "^8.19.1",
    "ajv": "^8.17.1",
    "cli-table": "^0.3.11",
    "dotenv": "^17.2.1",
    "pg": "^8.16.3",
    "pg-connection-string": "^2.9.1",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/chai": "^5.2.2",
    "@types/chai-as-promised": "^8.0.2",
    "@types/cli-table": "^0.3.4",
    "@types/mocha": "^10.0.10",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^22.17.1",
    "@types/sinon": "^17.0.4",
    "@types/sinon-chai": "^4.0.0",
    "chai": "^5.2.1",
    "chai-as-promised": "^8.0.1",
    "eslint": "^9.33.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "mocha": "^11.7.1",
    "mock-fs": "^5.5.0",
    "nyc": "^17.1.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.6.2",
    "public-ip": "^7.0.1",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.0",
    "tsx": "^4.20.4",
    "typedoc": "^0.28.10",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "engines": {
    "node": ">=20.19.0 || >=22.12.0"
  },
  "exports": {
    ".": "./dist/src/index.js",
    "./benchmark/*": "./dist/src/benchmark/*.js",
    "./database/*": "./dist/src/database/*.js",
    "./metrics/*": "./dist/src/metrics/*.js",
    "./parser/*": "./dist/src/parser/*.js",
    "./salesforce/*": "./dist/src/salesforce/*.js",
    "./scripts/*": "./dist/src/scripts/*.js",
    "./service/*": "./dist/src/service/*.js",
    "./state/*": "./dist/src/state/*.js"
  }
}

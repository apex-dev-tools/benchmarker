{
  "check-coverage": true,
  "extension": [
    ".ts"
  ],
  "exclude": [
    "**/*.test.ts",
    "**/*.d.ts",
    "scripts/apexLoader.ts",
    "src/shared/uiHelper.ts",
    "src/services/navigate/**/*.ts"
  ],
  "functions": 80,
  "instrument": true,
  "lines": 80,
  "per-file": true,
  "reporter": [
    "text",
    "text-summary"
  ],
  "sourceMap": true,
  "statements": 80
}

{
  "name": "@apexdevtools/benchmarker",
  "version": "1.0.0-beta.1",
  "description": "Benchmarks performance of processes on Salesforce Orgs",
  "author": {
    "name": "Apex Dev Tools Team",
    "email": "apexdevtools@gmail.com",
    "url": "https://github.com/apex-dev-tools"
  },
  "repository": "apex-dev-tools/benchmarker",
  "homepage": "https://github.com/apex-dev-tools/benchmarker",
  "main": "dist/src/index.js",
  "types": "dist/types/src/index.d.ts",
  "files": [
    "dist/src",
    "dist/types/src",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run compile && npm run build:copy",
    "build:clean": "rm -rf dist",
    "build:copy": "copyfiles -u 1 src/**/*.cls dist/src",
    "compile": "tsc --skipLibCheck",
    "doc:generate": "typedoc --excludeNotExported --ignoreCompilerErrors --out doc src/**/*",
    "lint": "tslint src/**/*.ts",
    "postversion": "npm run doc:generate",
    "test": "DEBUG=-* nyc mocha 'test/**/*.test.ts'",
    "test:only": "DEBUG=-* nyc mocha",
    "test:system:init": "cp test_system/.env .env",
    "test:system": "mocha 'test_system/**/*.test.ts' --timeout 30000"
  },
  "dependencies": {
    "@jsforce/jsforce-node": "3.2.0",
    "@salesforce/core": "7.3.5",
    "axios": "1.6.8",
    "cli-table": "0.3.1",
    "debug": "4.1.1",
    "dotenv": "16.0.3",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "pg": "8.7.3",
    "pg-connection-string": "2.0.0",
    "puppeteer": "19.11.1",
    "typeorm": "0.2.21"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "1.89.0",
    "@types/chai": "4.1.7",
    "@types/chai-as-promised": "7.1.0",
    "@types/cli-table": "0.3.0",
    "@types/debug": "4.1.4",
    "@types/dotenv": "4.0.3",
    "@types/lodash": "4.14.144",
    "@types/mocha": "7.0.2",
    "@types/node": "10.14.7",
    "@types/sinon": "5.0.5",
    "@types/sinon-chai": "3.2.0",
    "axios-mock-adapter": "1.21.4",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "copyfiles": "2.4.1",
    "lighthouse": "5.1.0",
    "mocha": "7.1.2",
    "nyc": "14.1.1",
    "sinon": "6.1.4",
    "sinon-chai": "3.2.0",
    "source-map-support": "0.5.12",
    "ts-node": "8.2.0",
    "tslint": "5.16.0",
    "typedoc": "0.17.2",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">16.0.0"
  },
  "nyc": {
    "branches": 80,
    "check-coverage": true,
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/shared/uiHelper.ts"
    ],
    "functions": 80,
    "include": [
      "src/shared/**/*.ts",
      "src/services/**/*.ts",
      "src/database/**/*.ts"
    ],
    "instrument": true,
    "lines": 80,
    "per-file": true,
    "reporter": [
      "html",
      "text",
      "text-summary"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "statements": 80
  },
  "lint-staged": {
    "*.ts": "tslint --fix src/**/*.ts"
  }
}

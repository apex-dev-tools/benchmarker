{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "curly": false,
    "indent": [
      true,
      "tabs"
    ],
    "interface-name": false,
    "max-line-length": [
      false
    ],
    "max-classes-per-file": [
      false,
      2
    ],
    "no-console": false,
    "no-unused-expression": false,
    "object-literal-sort-keys": false,
    "only-arrow-functions": false,
    "ordered-imports": false,
    "trailing-comma": false,
    "quotemark": [
      true,
      "single"
    ],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "allow-trailing-underscore"
    ],
    "no-namespace": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  }
}
